package br.com.alunocadastrados.collegio;

import java.util.Scanner;

public class CadastraAluno {
	
	private Pais pais;
	private Filhos estudante;
	
	
	public void cadastroListaDeEspera(Scanner leitor) {
		
		this.pais.setNome(leitor.next());
		this.pais.setCpf(leitor.nextLine());
		this.pais.setDataNasc(leitor.nextLine());
		
		DadosParaColegio dc = new DadosParaColegio();
		
		dc.setNomePais();
			
		
	}

}
