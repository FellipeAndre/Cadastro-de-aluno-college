package br.com.alunocadastrados.collegio;

public class Filhos extends Pessoa{
	
	private Pais dadosDoResponsavel;
	private String rg;
	private 
	
	
	public Pessoa getInformacaoFilho() {
		return informacaoFilho;
	}
	public void setInformacaoFilho(Pessoa informacaoFilho) {
		this.informacaoFilho = informacaoFilho;
	}
	public DadosPessoais getDadosFilhos() {
		return dadosFilhos;
	}
	public void setDadosFilhos(DadosPessoais dadosFilhos) {
		this.dadosFilhos = dadosFilhos;
	}
	
}
