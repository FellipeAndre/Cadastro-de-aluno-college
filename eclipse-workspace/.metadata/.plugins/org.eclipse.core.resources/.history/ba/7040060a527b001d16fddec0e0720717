package desafiosUdemy;

public class Jantar {

	public static void main(String[] args) {
		
		Pessoa p1 = new Pessoa();
		p1.nome = "Felipe Tobias";	
		p1.pesoPessoa = 146.67;
		
		Comida c1 = new Comida();
		Comida c2 = c1;
		c1.nome = "arroz";
		c2.pesoComida = 0.088;
		
	   
	   c1.nome = "feijao";
	   c1.pesoComida = 0.000;
	   
	   c2.pesoComida = 1.000;
	   
	  System.out.println(c2.pesoAposAlimentacao(p1.pesoPessoa));
		
		
	}
}
